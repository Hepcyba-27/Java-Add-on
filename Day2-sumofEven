package prg;

public class sumofEven {
	public static void main(String[] args) {
		int[] number=new int[5];
		number[0] = 5;
		number[1] =4;
		number[2] = 3;
		number[3] = 2;
		number[4] =1;
		System.out.println(number[3]);
		System.out.println(number.length);
			
			for(int i=number.length-1;i>=0;i--) {
				System.out.println(number[i]);
			}
		}
	}
